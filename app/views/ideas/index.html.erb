<div class="container">
  <div class="well">
    <h1>Ideas</h1>
  </div>
    <ul>
      <% @ideas.each do |idea| %>
      <div class="well">
        <li> <%= idea.title %><br>
        Idea: <%= idea.body %><br>
        Quality: <%= idea.quality %></li>
      </div>
      <% end %>
    </ul>
  </div>
</div>
